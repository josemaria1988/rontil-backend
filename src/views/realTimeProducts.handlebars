<h1>Productos en tiempo real</h1>
<ul id="lista-productos"></ul>
<form>
    <label for="title">Título:</label>
    <input type="text" id="title" name="title"><br>
    <label for="description">Descripción:</label>
    <input type="text" id="descripcion" name="descripcion"><br>
    <label for="code" id="code" name="code">Código:</label>
    <input type="text" id="code" name="code"><br>
    <label for="price">Precio:</label>
    <input type="number" id="price" name="price"><br>
    <label for="stock" id="stock" name="stock">Stock:</label>
    <input type="number" id="stock" name="stock"><br>
    <label for="category" id="category" name="category">Categoría:</label>
    <input type="text" id="category" name="category"><br>
    <button type="button" id="btnCrearProducto">Crear producto</button>
</form>
<script src="/socket.io/socket.io.js"></script>
<script type="module" src="/js/realTimeProducts.js"></script>